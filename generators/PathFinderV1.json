{
  "_version": "1",
  "name": "Path Finder V1",
  "generator_version": {
    "major": 1,
    "minor": 1,
    "micro": 0
  },
  "properties": [
  ],
  "program": {
    "exec": "PathFinderV1Gen",
    "units": "--units $$",
    "output": "--outfile $$",
    "robot": "--robotfile $$",
    "paths": "--pathfile $$",
    "timestep": "--timestep $$",
    "parameters": [
      {
        "name": "Step",
        "description": "The number of steps between waypoints",
        "type": "double",
        "arg": "--step $$",
        "default": 1000
      }
    ]
  }
}
