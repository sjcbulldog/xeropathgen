{
  "_version": "1",
  "name": "Xero Gen V1 - Trapezoidal (Experimental)",
  "generator_version": {
    "major": 1,
    "minor": 2,
    "micro": 0
  },
  "properties": [
    "distance_constraint",
    "nonzero_start_end_velocity"
  ],
  "program": {
    "exec": "XeroGenV1",
    "output": "--outfile $$",
    "parameters": [
      {
        "arg": "--diststep $$",
        "default": 2.0,
        "description": "The maximum distance between points internally when generating the path",
        "name": "diststep",
        "type": "double"
      }
    ],
    "paths": "--pathfile $$",
    "robot": "--robotfile $$",
    "timestep": "--timestep $$",
    "units": "--units $$",
    "others": "--scurve false"
  }
}
