{
  "name": "Xero Gen V1",
  "properties": [
    "distance_constraint",
    "nonzero_start_end_velocity"
  ],
  "program": {
    "exec": "XeroGenV1",
    "output": "--outfile $$",
    "parameters": [
      {
        "arg": "--diststep $$",
        "default": 2.0,
        "description": "The maximum distance between points internally when generating the path",
        "name": "diststep",
        "type": "double"
      },
      {
        "arg": "--scurve $$",
        "default": "false",
        "description": "Use SCurve (vs Trapezoidal) trajectory velocity profile",
        "name": "scurve",
        "type": "strings",
        "choices" : ["true" , "false"]
      }
    ],
    "paths": "--pathfile $$",
    "robot": "--robotfile $$",
    "timestep": "--timestep $$",
    "units": "--units $$"
  }
}
